{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "vijay11"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureSqlDatabase_properties_typeProperties_connectionString": {
			"type": "string"
		},
		"AzureSqlDatabase_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/848bffad-a9ae-486f-a160-76388acf2e31/resourceGroups/rg/providers/Microsoft.Sql/servers/adfdbserver1"
		},
		"AzureSqlDatabase_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"AzureSqlDatabase_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"adfdbserver1.database.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "vijay11_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureSqlDatabase_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase_properties_groupId')]"
					},
					"AzureSqlDatabase_properties_fqdns": {
						"value": "[parameters('AzureSqlDatabase_properties_fqdns')]"
					},
					"AzureSqlDatabase_properties_typeProperties_connectionString": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}